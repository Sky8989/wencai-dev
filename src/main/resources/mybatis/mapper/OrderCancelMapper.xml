<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.1//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sftc.web.dao.mybatis.OrderCancelMapper">

    <select id="selectCanceledOrderList" parameterType="com.sftc.web.model.entity.OrderCancel"
            resultType="com.sftc.web.model.entity.OrderCancel">
        SELECT * FROM c_order_cancel
        <where>
            <if test="express_id != null and express_id != 0">and express_id = #{express_id}</if>
        </where>
    </select>
    <select id="selectCanceleOrderByExpressId" parameterType="int" resultType="int">
        SELECT id
        FROM c_order_cancel
        WHERE express_id = #{expressId}
    </select>
</mapper>