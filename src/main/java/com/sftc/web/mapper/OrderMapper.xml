<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.1//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sftc.web.mapper.OrderMapper">

    <insert id="insertOrder" parameterType="com.sftc.web.model.Order">
        insert into sftc_order
        (gmt_order_create,gmt_pay_create,pay_method,freight,sender_name,
        sender_mobile,sender_province,sender_city,
        sender_area,sender_addr,ship_name,ship_mobile,ship_province,
        ship_city,ship_area,ship_addr,memos,type,size,user_id)
        values
        (#{gmt_order_create},#{gmt_pay_create},#{pay_method},#{freight},
        #{sender_name},#{sender_mobile},#{sender_province},#{sender_city},
        #{sender_area},#{sender_addr},#{ship_name},#{ship_mobile},#{ship_province},
        #{ship_city},#{ship_area},#{ship_addr},#{memos},#{type},#{size},#{user_id})

    </insert>
</mapper>