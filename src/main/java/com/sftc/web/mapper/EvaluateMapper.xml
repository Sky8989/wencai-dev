<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.1//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sftc.web.mapper.EvaluateMapper">
        <insert id="addEvaluate" parameterType="com.sftc.web.model.Evaluate">
            INSERT into sftc_evaluate (comments,fast_delivery,service_attitude,door_speed,order_id,user_id)
            VALUES (#{comments},#{fast_delivery},#{service_attitude},#{door_speed},#{order_id},#{user_id})
        </insert>

</mapper>